<?xml version="1.0"?>
<launch>

  <!-- Start Gazebo Simulator -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <!-- <arg name="world_name" value="$(find dataset_collector)/worlds/base.world"/> -->
    <!-- <arg name="world_name" value="$(find dataset_collector)/worlds/terrain_1.world"/> -->
    <!-- <arg name="world_name" value="$(find dataset_collector)/worlds/terrain_2.world"/> -->
    <arg name="world_name" value="$(find dataset_collector)/worlds/terrain_3.world"/>
    <!-- <arg name="world_name" value="$(find dataset_collector)/worlds/terrain_4.world"/> -->
    <!-- <arg name="world_name" value="$(find dataset_collector)/worlds/terrain_5.world"/> -->
    <!-- <arg name="world_name" value="$(find dataset_collector)/worlds/terrain_6.world"/> -->
  </include>
  
  <!-- Start RViz -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find dataset_collector)/rviz/config.rviz"/>

  <!-- tf -->
  <!-- basic -->
  <node pkg="tf" type="static_transform_publisher" name="world_to_camera_base" args="0 0 10 0 0 0 /world /camera_base 100"/>
  <node pkg="tf" type="static_transform_publisher" name="camera_base_to_camera1" args="12 12 0 -1.5708 0 3.141592654 /camera_base /camera1 100"/>
  <node pkg="tf" type="static_transform_publisher" name="camera_base_to_camera2" args="-12 12 0 -1.5708 0 3.141592654 /camera_base /camera2 100"/>
  <node pkg="tf" type="static_transform_publisher" name="camera_base_to_camera3" args="-12 -12 0 -1.5708 0 3.141592654 /camera_base /camera3 100"/>
  <node pkg="tf" type="static_transform_publisher" name="camera_base_to_camera4" args="12 -12 0 -1.5708 0 3.141592654 /camera_base /camera4 100"/>
  <node pkg="tf" type="static_transform_publisher" name="camera_base_to_camera5" args="0 0 0 -1.5708 0 3.141592654 /camera_base /camera5 100"/>


